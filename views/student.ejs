<h1> welcome to student page</h1>

<h1>Enter Student Details</h1>
<form action="/student/create-student-data" method="POST">
    <label for="name">Student Name :</label>
     <input type="text" name="name" required>
     <br>
     <label for="email">Email :</label>
     <input type="email" name="email" id="" required>
     <br>
     <label for="contact">Contact no:</label>
     <input type="number" name="contact" id="" maxlength="10" required>
     <br>
     <label for="batch">batch :</label>
     <input list="batch" name="batch" id="">
     <datalist id = "batch">
        <option value="10thOct"></option>
        <option value="12thsept"></option>
        <option value="14thNov"></option>
     </datalist>
     <br>
     <label for="college">College :</label>
     <input type="text" name="college" id="" required>
     <input type="submit" value="submit" name="" id="">

</form>